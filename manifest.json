{
  "name": "Curiosity",
  "description": "Maximize your rabbit holes",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "commands": {
    "create-search-tab": {
      "suggested_key": {
        "default": "Alt+T"
      },
      "description": "Create a new search tab"
    }
  }
}
